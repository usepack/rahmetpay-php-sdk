{
    "name": "usepack/rahmetpay-php-sdk",
    "description": "RahmetPay's PHP SDK",
    "keywords": ["rahmetpay", "payments", "rest", "sdk"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "rahmet-dev",
            "homepage": "https://github.com/usepack/rahmetpay-php-sdk/contributors"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "^7",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35 || ^5.7",
        "mockery/mockery": "^0.9.9",
        "friendsofphp/php-cs-fixer": "^2.17"
    },
    "autoload": {
        "psr-0": {
            "RahmetPay\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\RahmetPay\\": "tests/"
        }
    }
}
